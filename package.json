{
  "name": "typenative",
  "version": "0.0.8",
  "description": "Build native applications using Typescript.",
  "type": "module",
  "bin": {
    "typenative": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start:compiler": "node ./bin/index",
    "start:script": "node ./bin/index --script",
    "test": "node ./bin/index --source test/test.ts --script",
    "release": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danisss9/typenative.git"
  },
  "author": "Dani Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danisss9/typenative/issues"
  },
  "homepage": "https://github.com/danisss9/typenative#readme",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.11.20",
    "@types/shelljs": "^0.8.15"
  },
  "dependencies": {
    "fs-extra": "^11.2.0",
    "inquirer": "^9.2.15",
    "shelljs": "^0.8.5",
    "typescript": "^5.3.3"
  }
}
