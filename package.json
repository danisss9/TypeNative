{
  "name": "typenative",
  "version": "0.0.10",
  "description": "Build native applications using Typescript.",
  "type": "module",
  "bin": {
    "typenative": "bin/index.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "start:compiler": "node ./bin/index",
    "start:script": "node ./bin/index --script",
    "test": "node ./bin/index --source test/test.ts --script",
    "test2": "node ./bin/index --source test/test2.ts --script",
    "test3": "node ./bin/index --source test/test3.ts --script",
    "test4": "node ./bin/index --source test/test4.ts --script",
    "release": "npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danisss9/typenative.git"
  },
  "author": "Dani Santos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danisss9/typenative/issues"
  },
  "homepage": "https://github.com/danisss9/typenative#readme",
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.15.21",
    "@types/shelljs": "^0.8.16"
  },
  "dependencies": {
    "fs-extra": "^11.3.0",
    "inquirer": "^12.6.1",
    "shelljs": "^0.10.0",
    "typescript": "^5.8.3"
  }
}
